    <section style='background-image: url("/static/images/hi-rise.jpg");'>
      <div class="container">
        <div class="col-lg-10 col-lg-offset-1 margin-top-1">
          <% for(var i=0; i<communities.length; i++) {%>
            <% if (communities[i]._id == user.user.activeCommunity) {%>
              <div id="panic-placeholder"></div>
              <div id="panic-placeholder-socket"></div>
            <%}%>
          <%}%>
        </div>
      </div>
      <div class="container">
        <div class="col-md-10 col-md-offset-1 text-center">
          <button type="button" data-toggle="modal" href="#" onclick="$('#nameDatabaseModal').modal({'backdrop': 'static'});" class="btn btn-primary btn-lg margin-top-2">Name Database</button>
          <button type="button" data-toggle="modal" href="#plateDatabaseModal" class="btn btn-primary btn-lg margin-top-2">Plate Database</button>
          <button type="button" data-toggle="modal" href="#firearmDatabaseModal" class="btn btn-primary btn-lg margin-top-2">Firearm Database</button>
          <% if (user.user.activeCommunity != '' && user.user.activeCommunity != null) {%>
          <button type="button" data-toggle="modal" href="#boloModal" class="btn btn-primary btn-lg margin-top-2" onclick="clearBoloForm()">Create BOLO</button>
          <% } else {%>
            <button type="button" data-toggle="modal" href="#nonCommunityBoloModal" class="btn btn-primary btn-lg margin-top-2 mock-disabled">Create BOLO</button>
            <% } %>
          <button type="button" data-toggle="modal" href="#10codesModal" class="btn btn-primary btn-lg margin-top-2">10-Codes</button>
          <button type="button" data-toggle="modal" href="#notepadModal" class="btn btn-primary btn-lg margin-top-2">Notepad</button>
        </div>
      </div>
      <hr class="margin-top-2" style="width: 88%;max-width: 100rem;">
      <div class="container">
        <div id="active-bolo-container" class="col-md-12 col-md-offset-0.75 margin-top-2">
        <% if (user.user.activeCommunity != '' && user.user.activeCommunity != null) {%>
            <span id="active-bolos" class="margin-right-1" style="font-size: 18px;">Active BOLOs:</span> <button type="button" data-toggle="modal" href="#boloModal" class="btn btn-primary btn-md pull-right margin-bottom-1" onclick="clearBoloForm()">Create BOLO</button>
            <table id="boloTable" class="table table-hover table-bordered">
              <thead>
                  <tr>
                      <th>Type</th>
                      <th>Location</th>
                      <th>Description</th>
                  </tr>
              </thead>
              <tbody>
                      <% if (bolos != null && bolos.length > 0) {%>
                  <% for(var i=0; i<bolos.length; i++) {%>
                          <tr id="<%=bolos[i]._id%>-row" class="gray-hover" data-toggle="modal" data-id="<%=i%>" data-target="#boloDetailModal" onclick="populateBoloDetails('<%=bolos[i]._id%>')">
                              <td id="<%=bolos[i]._id%>-type" style="text-transform: capitalize;"><%=bolos[i].bolo.boloType%></td>
                              <td id="<%=bolos[i]._id%>-location"><%=bolos[i].bolo.location%></td>
                              <td id="<%=bolos[i]._id%>-description"><%=bolos[i].bolo.description%></td>
                          </tr>
                  <%}%>
                  <%}%>
              </tbody>
          </table>
        <% } %>
        </div>
        <div class="col-lg-10 col-lg-offset-1 margin-top-2">
          <% if (commUsers == null) {%>
            <h4>Looks like you are not in a community, click here to get started!</h4>
            <p><a data-toggle="modal" href="#communitiesModal">Create/Join A Community</a></p>
            <%} else {%>
          <h4 id="officer-list">Officer List:</h4>
          <table id="example" class="table table-hover table-bordered">
            <thead>
                <tr>
                    <th>On/Off Duty</th>
                    <th>Username</th>
                    <th>Status</th>
                </tr>
            </thead>
            <tbody>
                <% for(var i=0; i<commUsers.length; i++) {%>
                        <tr class="gray-hover" data-toggle="modal" data-id="<%=i%>" data-target="#manageUserDispatchStatusModal" onclick="setDispatchModalValues('<%=commUsers[i]._id%>')">
                          <% if (!commUsers[i].user.dispatchOnDuty) {%>
                            <td><span title="100" id="<%=commUsers[i]._id%>-duty" class="red-dot"></span></td>
                          <%} else if (commUsers[i].user.dispatchOnDuty) {%>
                          <td><span title="1" id="<%=commUsers[i]._id%>-duty" class="green-dot"></span></td>
                          <%}%>
                            
                            <td style="text-transform: capitalize;"><%=commUsers[i].user.username%></td>
                            <% if (commUsers[i].user.dispatchStatus == '' || commUsers[i].user.dispatchStatus == null) {%>
                              <td id="<%=commUsers[i]._id%>-status">-</td>
                              <%} else {%>
                                <% if (commUsers[i].user.dispatchStatus == 'Panic') {%>
                                  <td id="<%=commUsers[i]._id%>-status">ðŸš¨ <%=commUsers[i].user.dispatchStatus%>ðŸš¨</td>
                            <%} else {%>
                              <td id="<%=commUsers[i]._id%>-status"><%=commUsers[i].user.dispatchStatus%></td>  
                              <%}%>
                            <%}%>
                        </tr>
                <%}%>
            </tbody>
        </table>
        <%}%>
        </div>
    </div>
  </section>
