  function openNameDatabase(registeredOwner, activeCommunityID) {
    if (registeredOwner==null||registeredOwner==undefined) {
      if (activeCommunityID.length==0) {
        return document.location.href=`name-search?firstName=&lastName=&dateOfBirth=&activeCommunityID=${activeCommunityID}`
      }
      return document.location.href=`name-search?firstName=&lastName=&activeCommunityID=${activeCommunityID}`
    }
    let nameAndDOB=registeredOwner.split(" | ")
    if (nameAndDOB.length!==2) {
      if (activeCommunityID.length==0) {
        return document.location.href=`name-search?firstName=&lastName=&dateOfBirth=&activeCommunityID=${activeCommunityID}`
      }
      return document.location.href=`name-search?firstName=&lastName=&activeCommunityID=${activeCommunityID}`
    }
    let firstNameLastName=nameAndDOB[0].split(" ")
    if (firstNameLastName.length!==2) {
      if (activeCommunityID.length==0) {
        return document.location.href=`name-search?firstName=&lastName=&dateOfBirth=&activeCommunityID=${activeCommunityID}`
      }
      return document.location.href=`name-search?firstName=&lastName=&activeCommunityID=${activeCommunityID}`
    }
    //check to see if we are in a community, if we are not, 
    //then we need to use the DOB to do the name search
    if (activeCommunityID.length==0) {
      let dob=nameAndDOB[1].split(" ")
      if (dob.length!==2) {
        return document.location.href=`name-search?firstName=${firstNameLastName[0].trim()}&lastName=${firstNameLastName[1].trim()}&dateOfBirth=&activeCommunityID=${activeCommunityID}`
      }
      return document.location.href=`name-search?firstName=${firstNameLastName[0].trim()}&lastName=${firstNameLastName[1].trim()}&dateOfBirth=${dob[1]}&activeCommunityID=${activeCommunityID}`
    }
    return document.location.href=`name-search?firstName=${firstNameLastName[0].trim()}&lastName=${firstNameLastName[1].trim()}&activeCommunityID=${activeCommunityID}`
  }
