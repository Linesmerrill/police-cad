  $(document).ready(function(){

    // personas cards using data-tables to filter and sort
    if (document.cookie.split(';').some((item) => item.includes('persona_icon=list'))) {
      showList()
    } else if (document.cookie.split(';').some((item) => item.includes('persona_icon=app'))) {
      showApps()
    } else {
      $('.dataTables_filter').hide();
      $('#app-icon-personas').toggleClass('active-icon')
    }
    //used to filter the personas cards
    var personasDataTable = $('#personas-table').DataTable({
    "bLengthChange": false,
    })
    $('#civilian-search').on("keyup", function() {
      personasDataTable.search(this.value).draw();
      var value = $(this).val().toLowerCase();
      $(".civ-thumbnails").filter(function() {
        $(this).toggle($(this).text().toLowerCase().indexOf(value) > -1)
      });
    })

    // vehicles table
    if (document.cookie.split(';').some((item) => item.includes('vehicle_icon=list'))) {
      showVehicleList()
    } else if (document.cookie.split(';').some((item) => item.includes('vehicle_icon=app'))) {
      showVehicleApps()
    } else {
      $('.dataTables_filter').hide();
      $('#vehicles-table-div').hide()
      $('#app-icon-vehicles').toggleClass('active-icon')
    }
    var vehicleDataTable = $('#vehicle-table').DataTable({
      "bLengthChange": false,
    })
    $('#vehicle-search').on("keyup", function() {
      vehicleDataTable.search(this.value).draw();
      var value = $(this).val().toLowerCase();
      $(".veh-thumbnails").filter(function() {
        $(this).toggle($(this).text().toLowerCase().indexOf(value) > -1)
      });
    })

  // firearms table
  if (document.cookie.split(';').some((item) => item.includes('firearm_icon=list'))) {
      showFirearmsList()
    } else if (document.cookie.split(';').some((item) => item.includes('firearm_icon=app'))) {
      showFirearmsApps()
    } else {
      $('.dataTables_filter').hide();
      $('#firearms-table-div').hide()
      $('#app-icon-firearms').toggleClass('active-icon')
    }
  var firearmDataTable = $('#firearm-table').DataTable({
    "bLengthChange": false,
  })
  $('#firearm-search').on("keyup", function() {
    firearmDataTable.search(this.value).draw();
    var value = $(this).val().toLowerCase();
    $(".firearm-thumbnails").filter(function() {
      $(this).toggle($(this).text().toLowerCase().indexOf(value) > -1)
    });
  })



  // check for license-notification
  if (document.cookie.split(';').some((item) => item.includes('license_notice=hidden'))) {
      hideLicenseNotice()
    } else {
      $('#license-notice').show();
    }

// displays the Update Username and Cancel buttons for updating the username
