// add in a delete function as it is not supported as of jQuery {whatever-version-we-have}
$.delete = function(url, data, callback, type){
 if ( $.isFunction(data) ){
   type = type || callback,
       callback = data,
       data = {}
 }

 return $.ajax({
   url: url,
   type: 'DELETE',
   success: callback,
   data: data,
   contentType: type
 });
}

  function loadCivData(index) {
