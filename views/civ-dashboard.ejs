function loadVehData(index) {
     $('#vehicleID').val(dbVehicles[index]._id)
     $('#plateVeh').val(dbVehicles[index].vehicle.plate.toUpperCase())

     // since only cars after 6/26/2020 will have this info, we need to check for empty values
     if (dbVehicles[index].vehicle.vin == "" || dbVehicles[index].vehicle.vin == undefined) {
       $('#vinVeh').val("")
       $('#no-existing-vin').show()
     } else {
       $('#vinVeh').val(dbVehicles[index].vehicle.vin.toUpperCase())
       $('#no-existing-vin').hide()
     }

     $('#modelVeh').val(dbVehicles[index].vehicle.model)
     $('#colorView').val(dbVehicles[index].vehicle.color)
     $('#validRegView').val(dbVehicles[index].vehicle.validRegistration)
     $('#validInsView').val(dbVehicles[index].vehicle.validInsurance)
     $('#roVeh').val(dbVehicles[index].vehicle.registeredOwner)
     $('#stolenView').val(dbVehicles[index].vehicle.isStolen)
   }

   function loadFirearmData(index) {
     $('#firearmID').val(dbFirearms[index]._id)
     $('#serial-number-details').val(dbFirearms[index].firearm.serialNumber)
     $('#weapon-type-details').val(dbFirearms[index].firearm.weaponType)
     //Because from the backend we already split the person_id from the person name and dob, we now
     //need to rejoin those values back together for #registeredOwner-details
     if (dbFirearms[index].firearm.registeredOwner == 'N/A') {
       $('#registeredOwner-details').val(`${dbFirearms[index].firearm.registeredOwner}`)
     } else {
       $('#registeredOwner-details').val(`${dbFirearms[index].firearm.registeredOwnerID}+${dbFirearms[index].firearm.registeredOwner}`)
     }
     $('#is-stolen-details').val(dbFirearms[index].firearm.isStolen)
   }

